{
	"settings": {
		"authentication": {
			"method": "basic",
			"certificate": "",
			"key": ""
		},
		"tls": {
			"validation": false
		},
		"verbosity": 1,
		"headers": {
			"content-type": "application/json",
			"Accept-Encoding": "application/json",
			"X-Allow-Overwrite": "true",
			"X-Avi-Version": "21.1.3"
		},
		"endpoint": "https://avi.lab.engyak.net"
	},
	"plays": {
		"get_version": {
			"uri": "/api/cluster/version",
			"description": "Fetch Avi Version",
			"method": "GET",
			"requiresbody": false,
			"requiresvariables": false,
			"variables": {}
		},
		"get_tls_profiles": {
			"uri": "/api/sslprofile",
			"description": "Fetch Managed TLS Profiles",
			"method": "GET",
			"requiresbody": false,
			"requiresvariables": false,
			"variables": {}
		},
		"get_tls_profile": {
			"uri": "/api/sslprofile/{{ id }}",
			"description": "Fetch Managed TLS Profile",
			"method": "GET",
			"requiresbody": false,
			"requiresvariables": true,
			"variables": {
				"id": false
			}
		},
		"create_tls_profile": {
			"uri": "/api/sslprofile",
			"description": "Create Managed SSL Profile",
			"method": "POST",
			"requiresbody": true,
			"requiresvariables": false
		},
		"update_tls_profile": {
			"uri": "/api/sslprofile/{{ id }}",
			"description": "Create Managed SSL Profile",
			"method": "PUT",
			"requiresbody": true,
			"requiresvariables": false
		},
		"get_app_profiles": {
			"uri": "/api/applicationprofile",
			"description": "Fetch Managed Application Profiles",
			"method": "GET",
			"requiresbody": false,
			"requiresvariables": false,
			"variables": {}
		},
		"get_app_profile": {
			"uri": "/api/applicationprofile/{{ id }}",
			"description": "Fetch Managed Application Profile",
			"method": "GET",
			"requiresbody": false,
			"requiresvariables": true,
			"variables": {
				"id": false
			}
		},
		"create_app_profile": {
			"uri": "/api/applicationprofile",
			"description": "Create Managed Application Profile",
			"method": "POST",
			"requiresbody": true,
			"requiresvariables": false
		},
		"update_app_profile": {
			"uri": "/api/applicationprofile/{{ id }}",
			"description": "Create Managed Application Profile",
			"method": "PUT",
			"requiresbody": true,
			"requiresvariables": true
		},
		"get_pools": {
			"uri": "/api/pool",
			"description": "Get Load Balancer Pool",
			"method": "GET"
		},
		"create_pool": {
			"uri": "/api/pool",
			"description": "Create Load Balancer Pool",
			"method": "POST",
			"requiresbody": true
		}
	},
